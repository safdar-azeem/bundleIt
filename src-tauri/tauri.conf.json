{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "bundleit",
  "version": "0.1.0",
  "identifier": "com.bundleit.app",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "bundleit",
        "minWidth": 1050,
        "minHeight": 700,
        "width": 1100,
        "height": 770
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "fs": {
      "requireLiteralLeadingDot": false
    }
  }
}